MERGE INTO TB_USER A
USING ( SELECT * FROM TB_USER_TEMP ) B
ON    ( LOWER(A.USER_ID) = LOWER(B.USER_ID)
WHEN  MATCHED THEN
      UPDATE SET A>USER_NM = B.USER_NM, A.DEPT_ID = B.DEPT_ID, A.JOB_TITLE = B.JOB_TITLE
WHEN  NOT MATHED THEN 
      INSERT (USER_ID, USER_NM, DEPT_ID, JOB_TITLE) VALUES (B.USER_ID, B.USER_NM, B.DEPT_ID, B.JOB_TITLE)
ELSE  IGNORE;

      
